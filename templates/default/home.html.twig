{% extends "base.html.twig" %}

{% block title %}
    Home
{% endblock %}

{% block body %}

   {% for event in events %}
         <div class="event">
              <h2>{{ event.title }}</h2>
              <p>{{ event.description }}</p>
              <p>Date: {{ event.datetimeStart|date("d/m/Y H:i") }}</p>
         </div>
    {% else %}
         <p>Aucun événement à venir.</p>
    {% endfor %}

{% endblock %}
